{
  "name": "lomap",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "test": "NODE_ENV=test knex seed:run && npm run jest",
    "jest": "jest --forceExit --detectOpenHandles --runInBand",
    "dev": "nodemon -r dotenv/config src/server.js",
    "start": "nodemon server",
    "rollback": "knex migrate:rollback",
    "populate": "knex migrate:latest && knex seed:run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "aws-sdk": "^2.648.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.2.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-jwt-permissions": "^1.3.3",
    "express-pino-logger": "^4.0.0",
    "express-validator": "^6.4.0",
    "http-errors": "^1.7.3",
    "ioredis": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "objection": "^2.1.3",
    "pg": "^7.18.2",
    "phin": "^3.4.1",
    "require-directory": "^2.1.1",
    "sanitize-html": "^1.23.0",
    "socket.io": "^2.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ],
    "moduleNameMapper": {
      "^@root(.*)$": "<rootDir>$1",
      "^@Roles$": "<rootDir>/src/routes/groups/models/Roles.js",
      "^@Users$": "<rootDir>/src/routes/users/models/User.js",
      "^@base$": "<rootDir>/src/models/Base.js",
      "^@util(.*)$": "<rootDir>/src/util$1",
      "^@services(.*)$": "<rootDir>/src/services$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "_moduleAliases": {
    "$root": ".",
    "$app": "./src/app.js",
    "$base": "./src/models/Base.js",
    "$util": "./src/util",
    "$services": "./src/services"
  },
  "devDependencies": {
    "jest": "^25.2.7",
    "supertest": "^4.0.2"
  }
}
